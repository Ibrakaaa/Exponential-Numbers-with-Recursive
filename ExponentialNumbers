import java.util.Scanner;
public class Main {

   public static int pow(int a, int b) {
       if (b == 0) {
           return 1;
       }
       int result =1;
       for(int i=1;i<=b;i++){
           result *= a;

        }
        return result;

    }
    public static void main(String[] args){
        System.out.println(pow(2,3));
    }
}





